 public static void main(String[] args) {
        HashMap<Integer, String> map = new HashMap<Integer, String>();
        //Put elements in Map
        map.put(1, "Mango");
        map.put(2, "Apple");
        map.put(3, "Banana");
        map.put(4, "Grapes");

        for (Integer i : map.keySet()) {//key set returns all keys of the map
            System.out.println(i + " " + map.get(i));
        }
    }
}
